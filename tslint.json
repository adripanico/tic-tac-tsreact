{
  "defaultSeverity": "error",
  "extends": [
    "tslint-react"
  ],
  "jsRules": {
  },
  "rules": {
    "member-access": false,
    "ordered-imports": false,
    "quotemark": false,
    "no-console": false,
    "semicolon": false,
    "jsx-no-lambda": false,
    "object-literal-sort-keys": true,
    "import-spacing": true,
    "ordered-imports": "lowercase-first",
    "max-line-length": [
      true,
      {
        "limit": 140,
        "ignore-pattern": "^import |^export {(.*?)}",
        "check-strings": true,
        "check-regex": true
      }
    ]
  },
  "rulesDirectory": [
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
